<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN' 'http://java.sun.com/j2ee/dtds/web-app_2.2.dtd'>
<web-app>
  <display-name>$servlet.displayName</display-name>
  <description>$servlet.description</description>
  <servlet>
    <servlet-name>$servlet.name</servlet-name>
    <servlet-class>$servlet.class</servlet-class>
    <load-on-startup _ifTrue="$servlet.init.onstartup">100</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>$servlet.name</servlet-name>
    <url-pattern>/$app.name/*</url-pattern>
  </servlet-mapping>
</web-app>
