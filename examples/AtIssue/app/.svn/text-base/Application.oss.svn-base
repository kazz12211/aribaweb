module=model.Issue {
    rank:0;
    trait:SearchDetail!;
    layout=InspectStack {
        object {
            @layout#Tabs {
                bindings: { contentClass:"tabContentWrapper noPadding"; }
                @layout=Notes#Form { zonePath:Notes; }
                @layout=Watchers#Stack { }
            }
        }
    }
    field=notes { class { bindings: {class:"noBox scrollTableWrapper"; title:null } } }
}
